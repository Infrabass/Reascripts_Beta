desc: !!!    FLASHMOB GEN - DO NOT DELETE    !!!

in_pin:left input
in_pin:right input
out_pin:left output
out_pin:right output

slider1:0<0,0>-Dummy

@init

db = -9;
freq = 20000;


@slider
  vol=2 ^ (db/6); 
  adj = 2.0*$pi*freq/srate;

@sample
sine = cos(pos);
sine *= vol;

spl0 = sine;
spl1 = sine;

pos=pos+adj;
(pos >= 2.0*$pi) ? pos -= 2.0*$pi;
